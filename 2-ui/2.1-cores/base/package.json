{
  "name": "@impos2/ui-core-base",
  "version": "1.0.0",
  "description": "POS UI Core Base",
  "author": "DexterYang",
  "private": true,
  "react-native": "./src/index.ts",
  "exports": {
    ".": {
      "react-native": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "web": "expo start --web",
    "build": "tsc --noEmit --skipLibCheck",
    "build-for-product": "rm -rf dist && tsc --skipLibCheck",
    "clean": "rm -rf .expo node_modules",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@impos2/kernel-core-base": "*",
    "@impos2/kernel-core-interconnection": "*",
    "@impos2/kernel-core-navigation": "*",
    "@reduxjs/toolkit": "^2.11.2",
    "redux-persist": "^6.0.0",
    "reselect": "^5.1.1"
  },
  "peerDependencies": {
    "react": "19.2.4",
    "react-native": "0.84.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "expo": "~55.0.4",
    "expo-status-bar": "~2.2.3",
    "react-dom": "19.2.4",
    "react-native-web": "^0.21.2",
    "typescript": "^5.9.3"
  }
}
