{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "strict": true
  },
  "include": ["src", "dev", "native"],
  "exclude": ["node_modules", "dist", ".vite"]
}
